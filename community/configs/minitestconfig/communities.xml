<?xml version="1.0" encoding="UTF-8"?>
<Communities>
  <Community Name="MiniTestConfig" >
    <!-- Uncomment this to explicitly define community manager, otherwise
         a member is randomly selected to be manager
    <Attribute ID="CommunityManager" Value="3ID" />
    -->
    <Entity Name="3ID" >
      <Attribute ID="Role" Value="Member" />
    </Entity>
    <Entity Name="1BDE" >
      <Attribute ID="Role" Value="Member" />
      <!-- The following attribute can be used to inhibit selection of agent to be
           community manager in the event the community-level attribute
           "CommunityManager" is not set.
      <Attribute ID="CanBeManager" Value="False" />
    -->
    </Entity>
    <Entity Name="3-69-ARBN" >
      <Attribute ID="Role" Value="Member" />
    </Entity>
    <Entity Name="MCCGlobalMode" >
      <Attribute ID="Role" Value="Member" />
    </Entity>
  </Community>
</Communities>